{
  "name": "nft-service",
  "version": "0.1.0",
  "description": "NFT Marketplace",
  "type": "module",
  "scripts": {
    "build": "npx tsc",
    "start-app": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/src/nft-app.js",
    "start-app-dev": "tsc-watch --onSuccess \"npm run start-app\"",
    "start-ipfs-dev": "tsc-watch --onSuccess \"npm run start-ipfs\"",
    "docs": "npx tsc && node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/src/docs/docsGenerator.js",
    "test": "mocha -r tsconfig-paths/register -r ts-node/register \"./src/{,/**/}*.test.ts\" --require test/*.spec.ts",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix"
  },
  "keywords": [
    "nft",
    "marketplace"
  ],
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "@tsconfig/node16": "^1.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.24",
    "@types/nodemailer": "^6.4.4",
    "@types/sharp": "^0.30.5",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "chai": "^4.3.6",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.0.0",
    "mocha-prepare": "^0.1.0",
    "mongo-unit": "^3.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.7.0",
    "tsc-watch": "^5.0.3",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@fastify/busboy": "^1.1.0",
    "@fastify/cors": "^8.1.0",
    "@fastify/formbody": "^7.1.0",
    "@types/node-schedule": "^2.1.0",
    "ajv-errors": "3.0.0",
    "async-mkdirp": "^1.2.7",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "connect-busboy": "^1.0.0",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.0",
    "ethers": "^5.7.1",
    "fastify": "^4.2.0",
    "fastify-file-upload": "^4.0.0",
    "fcm-node": "^1.6.1",
    "files-from-path": "^0.2.6",
    "ioredis": "^5.1.0",
    "ipfs": "^0.64.2",
    "ipfs-core": "^0.16.1",
    "moment": "^2.29.4",
    "mongoose": "^6.3.0",
    "nft.storage": "^7.0.0",
    "node-schedule": "^2.1.0",
    "nodemailer": "^6.7.7",
    "sharp": "^0.31.0",
    "web3": "^1.8.0",
    "web3-providers-ws": "^1.8.1"
  },
  "author": "",
  "license": "ISC",
  "private": true
}
